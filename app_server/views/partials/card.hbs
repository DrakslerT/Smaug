
{{#each card}}
<div class="col-xl-3 col-lg-4 col-md-6 col-sm-12" data-aos="flip-down" data-aos-duration="500" style="margin: 0 auto">
    <div class="card-counter  {{color}}">
        <i class="fas {{icon}}"></i>
        <p class="count-numbers">{{count}}</p>
        <p class="count-name">{{title}}</p>
    </div>
</div>
{{/each}}
