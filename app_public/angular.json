{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "SmaugBudget": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/SmaugBudget",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets/images",
              "src/assets/stylesheets/fonts.css",
              "src/assets/stylesheets/main.css",
              "src/manifest.webmanifest",
              "src/assets/stylesheets/darkmode.css",
              "src/assets/stylesheets/_animations.scss",
              "src/assets/stylesheets/_core.scss",
              "src/assets/stylesheets/_easing.scss",
              "src/assets/stylesheets/aos.scss",
              "src/assets/javascripts/aos.js",
              "src/assets/javascripts/helpers/calculateOffset.js",
              "src/assets/javascripts/helpers/detector.js",
              "src/assets/javascripts/helpers/elements.js",
              "src/assets/javascripts/helpers/handleScroll.js",
              "src/assets/javascripts/helpers/prepare.js",
              "src/assets/javascripts/jquery-3.5.1.min.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/stylesheets/fonts.css",
              "src/assets/stylesheets/main.css",
              "src/assets/webfonts/OpenSans-Bold.ttf",
              "src/assets/webfonts/OpenSans-Italic.ttf",
              "src/assets/webfonts/OpenSans-Light.ttf",
              "src/assets/webfonts/OpenSans-Regular.ttf"
            ],
            "scripts": [
              "src/assets/javascripts/Dictionary.js",
              "src/assets/javascripts/globalFunctions.js",
              "src/assets/javascripts/bills.js",
              "src/assets/javascripts/confirmation.js",
              "src/assets/javascripts/DarkMode.js",
              "src/assets/javascripts/dashboard.js",
              "src/assets/javascripts/DashboardChart.js",
              "src/assets/javascripts/envelopes.js",
              "src/assets/javascripts/functions.js",
              "src/assets/javascripts/goals.js",
              "src/assets/javascripts/history.js",
              "src/assets/javascripts/HistoryChart.js",
              "src/assets/javascripts/index.js",
              "src/assets/javascripts/passwordReset.js",
              "src/assets/javascripts/utilities.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }, 
            "docker": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.docker.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "SmaugBudget:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "SmaugBudget:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "SmaugBudget:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "SmaugBudget:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "SmaugBudget:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "SmaugBudget"
}
